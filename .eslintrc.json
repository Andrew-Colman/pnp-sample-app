{
    "parser": "babel-eslint",
    "plugins": [
        "prettier",
        "react"
    ],
    "rules": {
        "prettier/prettier": [ "error", {
            "singleQuote": true,
            "trailingComma": "all",
            "bracketSpacing": false,
            "printWidth": 120,
            "parser": "babylon"
        } ]
    }
}
